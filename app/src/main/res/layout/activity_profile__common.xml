<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile_Common">

    <Button
        android:id="@+id/lookForCaterers"
        android:layout_width="274dp"
        android:layout_height="99dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="205dp"
        android:layout_marginEnd="69dp"
        android:layout_marginRight="69dp"
        android:onClick="lookForCaterer"
        android:text="Looking for Caterers"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/toCater"
        android:layout_width="274dp"
        android:layout_height="99dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="69dp"
        android:layout_marginRight="69dp"
        android:text="Looking to cater"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lookForCaterers" />

</android.support.constraint.ConstraintLayout>